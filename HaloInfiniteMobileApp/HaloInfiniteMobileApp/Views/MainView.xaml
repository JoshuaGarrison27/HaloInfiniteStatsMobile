<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:views="clr-namespace:HaloInfiniteMobileApp.Views"
                    Title="Halo Infinite"
                    xmlns:utility="clr-namespace:HaloInfiniteMobileApp.Utilities;assembly=HaloInfiniteMobileApp"
                    utility:ViewModelLocator.AutoWireViewModel="True"
                    x:Class="HaloInfiniteMobileApp.Views.MainView">
    <FlyoutPage.Flyout>
        <views:MenuView BindingContext="{Binding MenuViewModel}" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <ContentPage></ContentPage>
    </FlyoutPage.Detail>
</FlyoutPage>